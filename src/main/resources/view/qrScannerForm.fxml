<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.controller.QrScannerFormController">
   <children>
      <ProgressBar fx:id="prograss" layoutX="72.0" layoutY="299.0" prefHeight="23.0" prefWidth="485.0" progress="0.0" />
      <Button layoutX="360.0" layoutY="337.0" mnemonicParsing="false" onAction="#startBtnOnAction" prefHeight="31.0" prefWidth="66.0" style="-fx-background-color: lightgreen;" text="Start" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button layoutX="444.0" layoutY="337.0" mnemonicParsing="false" onAction="#stopBtnOnAction" prefHeight="31.0" prefWidth="66.0" style="-fx-background-color: darkred;" text="Stop" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <AnchorPane fx:id="pane" layoutX="22.0" layoutY="21.0" prefHeight="251.0" prefWidth="557.0">
         <children>
            <ImageView fx:id="imageView" fitHeight="237.0" fitWidth="557.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
      <Label fx:id="lbltext" layoutX="22.0" layoutY="333.0" prefHeight="38.0" prefWidth="292.0">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <Button layoutX="526.0" layoutY="337.0" mnemonicParsing="false" onAction="#backBtnOnAction" style="-fx-background-color: grey;" text="Back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
